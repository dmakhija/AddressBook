@model IEnumerable<AddressBook.Models.Role>

@{
    ViewBag.Title = "Index";
}

<h2>Roles</h2>


<table class="table table-striped table-responsive">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.RoleName)
        </th>
        
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.RoleName)
        </td>        
    </tr>
}

</table>
